<UserControl x:Class="video_mush_gui_new.trackingComboBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:video_mush_gui_new"
             mc:Ignorable="d" 
             d:DesignHeight="22" d:DesignWidth="300">

    <ComboBox x:Name="box" Height="22" VerticalAlignment="Top" Margin="0,0,-0,-0" SelectionChanged="box_SelectionChanged">
        <ComboBoxItem IsSelected="True" Visibility="Hidden"/>
        <ComboBoxItem Content="Choose Path..."/>
        <Separator/>
    </ComboBox>

</UserControl>
